# vim: ft=make

#
#  $< represents the first prerequisiste
#

all: dep_1 dep_2 dep_3
	@echo all, $$\< = $<

dep_1: dep_1_a dep_1_b dep_1_c
	@echo dep_1, $$\< = $<

dep_2: dep_2_y dep_2_z
	@echo dep_2, $$\< = $<

dep_1_a:
	@echo dep_1_a, $$\< = $<

dep_1_b:
	@echo dep_1_b, $$\< = $<

dep_1_c:
	@echo dep_1_c, $$\< = $<

dep_2_y:
	@echo dep_2_y, $$\< = $<

dep_2_z:
	@echo dep_2_z, $$\< = $<

dep_3:
	@echo dep_3, $$\< = $<
